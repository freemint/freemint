#
# debug flags
#
# -DDEV_DEBUG		include debugging information


ifeq ($(xconout2),deb)
TARGET = xconout2.xdd
CPU = 020-60
XCONOUT2DEFS += -DDEV_DEBUG
endif

ifeq ($(xconout2),000)
TARGET = xconout2.xdd
CPU = 000
XCONOUT2DEFS +=
endif

ifeq ($(xconout2),02060)
TARGET = xconout2.xdd
CPU = 020-60
XCONOUT2DEFS +=
endif

ifeq ($(xconout2),col)
TARGET = xconout2.xdd
CPU  = v4e
XCONOUT2DEFS +=
endif

#
# all default targets
#
xconout2targets = deb 000 02060 col
