#
# debug flags
#
# -DDEV_DEBUG		include debugging information


ifeq ($(lp),deb)
TARGET = lp.xdd
CPU = 020-60
LPDEFS += -DDEV_DEBUG
endif

ifeq ($(lp),000)
TARGET = lp.xdd
CPU = 000
LPDEFS +=
endif

ifeq ($(lp),02060)
TARGET = lp.xdd
CPU = 020-60
LPDEFS +=
endif

ifeq ($(lp),col)
TARGET = lp.xdd
CPU  = v4e
LPDEFS +=
endif

#
# all default targets
#
lptargets = deb 000 02060 col
