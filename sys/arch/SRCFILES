	detect.spp \
	intr.spp \
	kernel.spp \
	mmu030.spp \
	mmu040.spp \
	slb_util.spp \
	syscall.spp \
	vm5.spp \
	scsidrv_emu.spp \
	xhdi_emu.spp

SRCFILES = $(STARTUP) $(COBJS) $(SPPOBJS) $(SOBJS)
