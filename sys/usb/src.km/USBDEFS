#
# debug flags
#
# -DDEV_DEBUG		include debugging information


ifeq ($(usb),deb)
TARGET = usbdeb.km
CPU = 020-60
USBDEFS += -DDEV_DEBUG
endif

#ifeq ($(usb),000)
#TARGET = usb000.km
#CPU = 000
#USBDEFS +=
#endif

ifeq ($(usb),02060)
TARGET = usb02060.km
CPU = 020-60
USBDEFS +=
endif

ifeq ($(usb),030)
TARGET = usb030.km
CPU = 030
USBDEFS +=
endif

ifeq ($(usb),040)
TARGET = usb040.km
CPU = 040
USBDEFS +=
endif

ifeq ($(usb),060)
TARGET = usb060.km
CPU = 060
USBDEFS +=
endif

