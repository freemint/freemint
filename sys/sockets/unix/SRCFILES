	dgram.h \
	stream.h \
	un.h \
	unix.h \
	unixdev.h

COBJS = \
	cache.c \
	dgram.c \
	stream.c \
	unix.c \
	unixdev.c

SRCFILES = $(HEADER) $(COBJS)
