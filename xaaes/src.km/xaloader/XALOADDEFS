ifeq ($(xaload),000)
TARGET = ./xaloader.prg
CPU = 000
endif

ifeq ($(xaload),02060)
TARGET = ./xaloader.prg
CPU = 020-60
endif

ifeq ($(xaload),030)
TARGET = ./xaloader.prg
CPU = 030
endif

ifeq ($(xaload),040)
TARGET = ./xaloader.prg
CPU = 040
endif

ifeq ($(xaload),060)
TARGET = ./xaloader.prg
CPU = 060
endif

ifeq ($(xaload),col)
TARGET = ./xaloader.prg
CPU = v4e
endif

xaloadtargets = 000 02060 030 040 060 col
