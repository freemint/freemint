ifeq ($(nfs),000)
TARGET = ./mount_nfs
CPU = 000
endif

ifeq ($(nfs),02060)
TARGET = ./mount_nfs
CPU = 02060
endif

ifeq ($(nfs),030)
TARGET = ./mount_nfs
CPU = 030
endif

ifeq ($(nfs),040)
TARGET = ./mount_nfs
CPU = 040
endif

ifeq ($(nfs),060)
TARGET = ./mount_nfs
CPU = 060
endif

ifeq ($(nfs),col)
TARGET = ./mount_nfs
CPU = v4e
endif


nfstargets = 000 02060 030 040 060 col
