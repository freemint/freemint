ifeq ($(sysctl),000)
TARGET = ./sysctl
CPU = 000
endif

ifeq ($(sysctl),02060)
TARGET = ./sysctl
CPU = 02060
endif

ifeq ($(sysctl),030)
TARGET = ./sysctl
CPU = 030
endif

ifeq ($(sysctl),040)
TARGET = ./sysctl
CPU = 040
endif

ifeq ($(sysctl),060)
TARGET = ./sysctl
CPU = 060
endif

ifeq ($(sysctl),col)
TARGET = ./sysctl
CPU = v4e
endif

sysctltargets = 000 02060 030 040 060 col
