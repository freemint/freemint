ifeq ($(fdisk),000)
TARGET = ./fdisk ./sfdisk
CPU = 000
endif

ifeq ($(fdisk),02060)
TARGET = ./fdisk ./sfdisk
CPU = 02060
endif

ifeq ($(fdisk),030)
TARGET = ./fdisk ./sfdisk
CPU = 030
endif

ifeq ($(fdisk),040)
TARGET = ./fdisk ./sfdisk
CPU = 040
endif

ifeq ($(fdisk),060)
TARGET = ./fdisk ./sfdisk
CPU = 060
endif

ifeq ($(fdisk),col)
TARGET = ./fdisk ./sfdisk
CPU = v4e
endif

fdisktargets = 000 02060 030 040 060 col
