ifeq ($(fsetter),000)
TARGET = ./fsetter.app
CPU = 000
endif

ifeq ($(fsetter),02060)
TARGET = ./fsetter.app
CPU = 020-60
endif

ifeq ($(fsetter),030)
TARGET = ./fsetter.app
CPU = 030
endif

ifeq ($(fsetter),040)
TARGET = ./fsetter.app
CPU = 040
endif

ifeq ($(fsetter),060)
TARGET = ./fsetter.app
CPU = 060
endif

ifeq ($(fsetter),col)
TARGET = ./fsetter.app
CPU = v4e
endif

fsettertargets = 000 02060 030 040 060 col
