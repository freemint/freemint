ifeq ($(mintload),000)
TARGET = ./mintload.prg
CPU = 000
endif

ifeq ($(mintload),02060)
TARGET = ./mintload.prg
CPU = 020-60
endif

ifeq ($(mintload),030)
TARGET = ./mintload.prg
CPU = 030
endif

ifeq ($(mintload),040)
TARGET = ./mintload.prg
CPU = 040
endif

ifeq ($(mintload),060)
TARGET = ./mintload.prg
CPU = 060
endif

ifeq ($(mintload),col)
TARGET = ./mintload.prg
CPU = v4e
endif

mintloadtargets = 000 02060 030 040 060 col
