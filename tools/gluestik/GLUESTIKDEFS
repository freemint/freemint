CPU = 030
endif

ifeq ($(gluestik),040)
TARGET = ./gluestik.prg
CPU = 040
endif

ifeq ($(gluestik),060)
TARGET = ./gluestik.prg
CPU = 060
endif

ifeq ($(gluestik),col)
