ifeq ($(mkfatfs),000)
TARGET = ./mkfatfs ./gemkfatfs.app
CPU = 000
endif

ifeq ($(mkfatfs),02060)
TARGET = ./mkfatfs ./gemkfatfs.app
CPU = 020-60
endif

ifeq ($(mkfatfs),030)
TARGET = ./mkfatfs ./gemkfatfs.app
CPU = 030
endif

ifeq ($(mkfatfs),040)
TARGET = ./mkfatfs ./gemkfatfs.app
CPU = 040
endif

ifeq ($(mkfatfs),060)
TARGET = ./mkfatfs ./gemkfatfs.app
CPU = 060
endif

ifeq ($(mkfatfs),col)
TARGET = ./mkfatfs ./gemkfatfs.app
CPU = v4e
endif

mkfatfstargets = 000 02060 030 040 060 col
