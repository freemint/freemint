TARGET = ./sysdir.tos
CPU = 02060
endif

ifeq ($(sysdir),030)
TARGET = ./sysdir.tos
CPU = 030
endif

ifeq ($(sysdir),040)
TARGET = ./sysdir.tos
CPU = 040
endif

ifeq ($(sysdir),060)
TARGET = ./sysdir.tos
CPU = 060
endif

ifeq ($(sysdir),col)
TARGET = ./sysdir.tos
CPU = v4e
endif


sysdirtargets = 000 02060 030 040 060 col
