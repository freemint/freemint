ifeq ($(nohog2),000)
TARGET = ./nohog2.acc
CPU = 000
endif

ifeq ($(nohog2),02060)
TARGET = ./nohog2.acc
CPU = 02060
endif

ifeq ($(nohog2),030)
TARGET = ./nohog2.acc
CPU = 030
endif

ifeq ($(nohog2),040)
TARGET = ./nohog2.acc
CPU = 040
endif

ifeq ($(nohog2),060)
TARGET = ./nohog2.acc
CPU = 060
endif

ifeq ($(nohog2),col)
TARGET = ./nohog2.acc
CPU = v4e
endif


nohog2targets = 000 02060 030 040 060 col
